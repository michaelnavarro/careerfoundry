<div class="container-inner">
	<h1>Form Input</h1>

	<%= form_tag(form_output_path, method: "get") do %>
	  <p>
	  	<%= label_tag(:name, "Name:") %>
	  	<%= text_field_tag(:name) %>
	  </p>
	  <p>
	  	<%= label_tag(:color, "Favorite Color:") %>
	  	<%= text_field_tag(:color) %>
	  </p>
	  <p>
	  	<%= submit_tag("Submit") %>
	  </p>
	<% end %>

</div>